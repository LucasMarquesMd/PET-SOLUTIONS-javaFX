<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="405.0" prefWidth="685.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ClienteListController">
   <children>
      <Label text="Cliente" />
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button fx:id="btnCadastrar" mnemonicParsing="false" onAction="#onBtnCadastrar" style="-fx-background-color: FFB6C1;" text="Cadastrar" />
            <TextField fx:id="txtNome" prefHeight="25.0" prefWidth="343.0" promptText="Consultar cliente (nome do cliente)" />
            <Button fx:id="btnConsultar" mnemonicParsing="false" onAction="#onBtnConsultar" style="-fx-background-color: FFB6C1;" text="Consultar" />
        </items>
      </ToolBar>
      <TableView fx:id="tableViewCliente" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="tableCollumnId" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="tableCollumnName" prefWidth="75.0" text="Nome" />
            <TableColumn fx:id="tableCollumnEmail" prefWidth="75.0" text="Email" />
            <TableColumn fx:id="tableCollumnCPF" prefWidth="75.0" text="CPF" />
            <TableColumn fx:id="tableCollumnTelefone" prefWidth="75.0" text="Telefone" />
            <TableColumn fx:id="tableCollumnCelular" prefWidth="75.0" text="Celular" />
            <TableColumn fx:id="tableCollumnEDIT" prefWidth="75.0" />
            <TableColumn fx:id="tableColumnREMOVE" prefWidth="75.0" />
        </columns>
      </TableView>
   </children>
</VBox>
